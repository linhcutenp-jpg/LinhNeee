local function findAllByName(name)
    local lower = (name or ""):lower()
    local found = {}
    if lower == "" then return found end
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj.Name:lower() == lower then
            table.insert(found, obj)
        end
    end
    return found
end

