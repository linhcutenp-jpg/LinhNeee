local AntiBringEnabled = false
local antiConn = nil
local lastPos = nil
local BRING_THRESHOLD = 60

local function startAntiBringLoop()
    if antiConn then antiConn:Disconnect(); antiConn = nil end
    if not (lp and lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")) then return end
    local hrp = lp.Character.HumanoidRootPart
    lastPos = hrp.Position
    antiConn = RunService.Heartbeat:Connect(function()
        if not AntiBringEnabled then lastPos = hrp.Position; return end
        if not hrp or not hrp.Parent then return end
        local cur = hrp.Position
        local d = (cur - lastPos).Magnitude
        if d > BRING_THRESHOLD then
            pcall(function() hrp.CFrame = CFrame.new(lastPos) end)
            warn("[AntiBring] reverted unexpected teleport (d="..tostring(math.floor(d))..")")
            lastPos = hrp.Position
        else
            lastPos = cur
        end
    end)
end

