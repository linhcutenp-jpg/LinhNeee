local goDebounce = false

local function findPartBest(name)
    if type(findFirstByName) == "function" then
        return findFirstByName(name)
    end
    local key = tostring(name or ""):lower()
    if key == "" then return nil end
    local best, bd = nil, math.huge
    local hrpPos = (lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") and lp.Character.HumanoidRootPart.Position) or Vector3.new()
    for _, v in ipairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and v.Name:lower():find(key,1,true) then
            local d = (v.Position - hrpPos).Magnitude
            if d < bd then bd = d; best = v end
        end
    end
    return best
end

local function GoToPartByName(name)
    if goDebounce then return false, "Busy" end
    goDebounce = true
    task.defer(function() task.wait(0.6); goDebounce = false end)

    if not (lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")) then return false, "No HRP" end
    local part = findPartBest(name)
    if not part then return false, "Part not found" end

    local dest = part.Position + Vector3.new(0,4,0)
    local hrp = lp.Character.HumanoidRootPart
    local ok, err = pcall(function()
        if TELEPORT_TWEEN_TIME > 0 then
            TweenService:Create(hrp, TweenInfo.new(TELEPORT_TWEEN_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {CFrame = CFrame.new(dest)}):Play()
        else
            hrp.CFrame = CFrame.new(dest)
        end
    end)
    if not ok then
        pcall(function() hrp.CFrame = CFrame.new(dest) end)
    end
    if lastPos ~= nil then
        lastPos = hrp.Position
    end
    return true, "Teleported to "..(part.Name or "Part")
end

