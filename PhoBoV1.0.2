local function createItemESP(obj)
    local adornee = obj:FindFirstChildWhichIsA("BasePart") or obj:FindFirstChild("Handle")
    if not adornee then return end
    local esp = Instance.new("BoxHandleAdornment")
    esp.Size = adornee.Size + Vector3.new(0.5,0.5,0.5)
    esp.Color3 = Color3.fromRGB(255,200,0)
    esp.Transparency = 0.3
    esp.AlwaysOnTop = true
    esp.ZIndex = 10
    esp.Adornee = adornee
    esp.Parent = adornee
    locateESPObjects[obj] = esp
end

