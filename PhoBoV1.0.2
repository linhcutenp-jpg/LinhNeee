    antiBtn.BackgroundColor3 = AntiBringEnabled and Color3.fromRGB(80,200,120) or Color3.fromRGB(90,90,100)
    if AntiBringEnabled then
        startAntiBringLoop()
    else
        if antiConn then antiConn:Disconnect(); antiConn = nil end
    end
end)

